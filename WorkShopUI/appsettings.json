{
  "WorkShopAPI": {
    "BaseURL": "${WORKSHOP_API_URL?http://localhost:8080}"

  },
  "Auth0": {
    "Domain": "${AUTH0_DOMAIN}",
    "ClientId": "${AUTH0_CLIENT_ID}",
    "ClientSecret": "${AUTH0_CLIENT_SECRET}",
    "AuthUrl": "${AUTH0_URL}",
    "Audience": "${AUTH0_AUDIENCE}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
